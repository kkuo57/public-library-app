<%= form_for @user do |newuser| %>
  <%= newuser.text_field :first_name, placeholder: 'First Name' %>
  <%= newuser.text_field :last_name, placeholder: 'Last Name' %>
  <%= newuser.text_field :email, placeholder: 'Email' %>
  <%= newuser.password_field :password, placeholder: 'Password' %>
  <%= newuser.submit 'Submit' %>
<% end %>